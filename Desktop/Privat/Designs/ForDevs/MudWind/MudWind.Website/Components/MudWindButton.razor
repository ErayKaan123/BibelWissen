@inherits MudButton

<MudButton Color="Color" 
           Disabled="Disabled" 
           Href="@Href" 
           Class="@ClassBuilder"
           DisableElevation="true" 
           DisableRipple="true"
           Size="@Size"
           FullWidth="@FullWidth"
           IconClass="@IconClass"
           StartIcon="@StartIcon"
           EndIcon="@EndIcon"
           Style="@Style"
           ButtonType="@ButtonType"
           IconSize="@IconSize"
           Rel="@Rel"
           IconColor="@IconColor"
           Target="@Target"
           Variant="@Variant"
           Tag="@Tag"
           HtmlTag="@HtmlTag"
           UserAttributes="@UserAttributes"
           ClickPropagation="@ClickPropagation"
           OnClick="@OnClick">
    @ChildContent
</MudButton>